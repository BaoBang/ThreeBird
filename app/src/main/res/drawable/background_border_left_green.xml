<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/color_white_90" />
        </shape>
    </item>

    <item android:bottom="@dimen/stroke_width" android:right="@dimen/stroke_width" android:top="@dimen/stroke_width">
        <shape android:shape="rectangle">
            <solid android:color="@color/green" />
        </shape>
    </item>
    <item android:left="@dimen/border_stroke_width" android:top="@dimen/stroke_width" android:right="@dimen/stroke_width" android:bottom="@dimen/stroke_width">
        <shape android:shape="rectangle">
            <solid android:color="@color/main" />
        </shape>
    </item>
</layer-list>